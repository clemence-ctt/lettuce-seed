<form style="display:inline-block" method="post" action="{{ path('dashboard_picture_delete', {'plantId' : selectedPlant.id, 'id': picture.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ picture.id) }}">
    <button class="button">Delete</button>
</form>
